#include <iostream>
#include <fstream>

using namespace std;

int main(){
    ifstream input;
    input.open("Input.txt");
    int horizontal = 0, depth = 0, param;
    string command;
    while(!input.eof()){
        input >> command >> param;
        if(command=="forward"){
            horizontal += param;
        }else if(command=="down"){
            depth += param;
        }else if(command=="up"){
            depth -= param;
        }
    }
    cout << horizontal * depth << endl;
    input.close();
}
