#include <iostream>
#include <fstream>

using namespace std;

int main(){
    ifstream input;
    input.open("Input.txt");
    int past, current, numInc = 0;
    input >> current;
    while(!input.eof()){
        past = current;
        input >> current;
        if(past<current){
            numInc++;
        }
    }
    input.close();
    cout << numInc << endl;
}
